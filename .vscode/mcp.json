{
	"servers": {
		"github": {
			"type": "http",
			"url": "https://api.githubcopilot.com/mcp/"
		},
		"io.github.ChromeDevTools/chrome-devtools-mcp": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"chrome-devtools-mcp@0.13.0"
			],
			"gallery": "https://api.mcp.github.com",
			"version": "0.13.0"
		},
		"MongoDB": {
			"command": "npx",
			"args": [
					"-y",
					"mongodb-mcp-server@latest"				
			],
			"env": {
				"MDB_MCP_CONNECTION_STRING": "mongodb://root:password@localhost:27017/restaurant?authSource=admin"       
			}
		}	
	},
	"inputs": []
}
